{
	"name": "motw-scaffold",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test": "vitest",
		"postinstall": "bun run db:generate",
		"db:generate": "prisma-emitter generate --schema=./prisma/schema.prisma --output=./src/lib/generated/prisma/"
	},
	"devDependencies": {
		"@eslint/js": "10.0.1",
		"@sveltejs/adapter-auto": "7.0.1",
		"@sveltejs/adapter-node": "5.5.3",
		"@sveltejs/kit": "2.53.1",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@testing-library/svelte": "5.3.1",
		"eslint": "10.0.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.15.0",
		"globals": "17.3.0",
		"prettier": "3.8.1",
		"prettier-plugin-svelte": "3.5.0",
		"prisma": "7.4.1",
		"prisma-json-types-generator": "4.1.1",
		"svelte": "5.53.3",
		"svelte-check": "4.4.3",
		"typescript": "5.9.3",
		"typescript-eslint": "8.56.1",
		"vite": "7.3.1",
		"vite-plugin-devtools-json": "1.0.0",
		"vitest": "4.0.18"
	},
	"dependencies": {
		"@discord/embedded-app-sdk": "2.4.0",
		"@prisma/adapter-pg": "7.4.1",
		"@prisma/client": "7.4.1",
		"@skeletonlabs/skeleton": "4.12.0",
		"@skeletonlabs/skeleton-svelte": "4.12.0",
		"@tailwindcss/vite": "4.2.1",
		"better-auth": "1.4.19",
		"prisma-extension-emitter": "2.1.14",
		"sveltekit-superforms": "2.30.0",
		"tailwindcss": "4.2.1",
		"zod": "4.3.6"
	},
	"overrides": {
		"@prisma/internals": "7.4.1"
	}
}